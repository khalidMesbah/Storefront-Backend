{
  "name": "storefront_backend",
  "version": "2.0.0",
  "description": "A RESTful JSON Node API backed by Postgres database for a shopping website.",
  "main": "./src/server.ts",
  "scripts": {
    "start": "npm run build && node dist/server.js",
    "dev": "nodemon src/server.ts",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess 'node ./dist/server.js'",
    "build": "tsc",
    "format": "prettier --write './src/**/*.ts'",
    "lint": "eslint './src/**/*.ts'",
    "lint:fix": "eslint './src/**/*.ts' --fix",
    "jasmine": "jasmine; exit 0",
    "test": "rm -r './dist' && tsc && export NODE_ENV=test && db-migrate up --env test && tsc && npm run jasmine && db-migrate reset -e test",
    "mig:u": "db-migrate up",
    "mig:d": "db-migrate down",
    "mig:r": "db-migrate reset"
  },
  "author": "Khalid Mesbah",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-rate-limit": "^6.3.0",
    "jsonwebtoken": "^8.5.1",
    "node": "^17.7.1",
    "pg": "^8.7.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^4.0.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "body-parser": "^1.19.2",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "helmet": "^5.0.2",
    "jasmine": "^4.0.2",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.4.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.0",
    "supertest": "^6.2.2",
    "ts-node": "^10.7.0",
    "tsc-watch": "^4.6.0",
    "typescript": "^4.6.2"
  }
}
